Transition {
  Lines   = Line*
  Line    = ( Positions | Step )

  // Extract the positions
  Positions  = PositionStart _ "->" _ PositionEnd endOfLine
  PositionStart = nameChar+
  PositionEnd = nameChar+

  comment = "#" anyChar*
  space += comment

  nameChar = ~endOfLine (letter | digit | "_" | " ")
  stepChar = (letter | " ")

  Step = "\t" digit "." _ stepChar+ 
  
  _ = (" " | "\t")*
  endOfLine = "\n" | "\r\n" | "\r" | end
  anyChar = ~endOfLine any
}